{
  "plugins": [
    {
      "name": "DynamicSynteny",
      "url": "http://localhost:9000/dist/jbrowse-plugin-dynamic-synteny.umd.development.js"
    }
  ],
  "assemblies": [
    {
      "name": "W",
      "aliases": ["Wm82"],
      "sequence": {
        "type": "ReferenceSequenceTrack",
        "trackId": "Wm82.gnm4-ReferenceSequenceTrack",
        "adapter": {
          "type": "BgzipFastaAdapter",
          "fastaLocation": {
            "uri": "https://data.soybase.org/Glycine/max/genomes/Wm82.gnm4.4PTR/glyma.Wm82.gnm4.4PTR.genome_main.fna.gz",
            "locationType": "UriLocation"
          },
          "faiLocation": {
            "uri": "https://data.soybase.org/Glycine/max/genomes/Wm82.gnm4.4PTR/glyma.Wm82.gnm4.4PTR.genome_main.fna.gz.fai",
            "locationType": "UriLocation"
          }
        }
      }
    }
  ],
  "tracks": [
    {
      "type": "SyntenyTrack",
      "trackId": "dynamic-synteny-track",
      "name": "Dynamic Synteny",
      "assemblyNames": ["W", "W"],
      "adapter": {
        "type": "DynamicSyntenyAdapter",
        "url": "http://localhost/gcv/microservices/macro-synteny-paf?genome1=glyma.Wm82.gnm4&genome2=glyma.Wm82.gnm4&matched=10&intermediate=5&mask=20&format=json",
        "appendRegionParams": false,
        "assemblyNames": ["W", "W"],
        "clientSideFilter": true,
        "cacheTimeout": 300000
      }
    },
    {
      "type": "SyntenyTrack",
      "trackId": "paf-synteny-track",
      "name": "PAF Synteny",
      "assemblyNames": ["W", "W"],
      "adapter": {
        "type": "PAFAdapter",
        "pafLocation": {
          "uri": "http://localhost/gcv/microservices/macro-synteny-paf?genome1=glyma.Wm82.gnm4&genome2=glyma.Wm82.gnm4&matched=10&intermediate=5&mask=20&format=paf"
        },
        "assemblyNames": ["W", "W"]
      }
    }
  ]
}